    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ViewMonthlySchedule">

    <TextView
        android:id="@+id/title"
        android:layout_width="406dp"
        android:layout_height="48dp"
